{
  "name": "@yeger/streams",
  "type": "module",
  "version": "1.4.0",
  "author": {
    "name": "Jan MÃ¼ller",
    "url": "https://github.com/DerYeger"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DerYeger/yeger.git",
    "directory": "packages/streams"
  },
  "bugs": {
    "url": "https://github.com/DerYeger/yeger/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./sync": {
      "import": {
        "types": "./dist/sync.d.ts",
        "default": "./dist/sync.js"
      }
    },
    "./async": {
      "import": {
        "types": "./dist/async.d.ts",
        "default": "./dist/async.js"
      }
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "bench": "vitest bench",
    "build": "tsdown",
    "check:publish": "publint run --strict",
    "check:tsc": "tsc --noEmit",
    "dev": "tsdown --watch",
    "lint": "yeger-lint",
    "test": "vitest"
  },
  "devDependencies": {
    "@yeger/tsconfig": "workspace:*",
    "@yeger/vitest-utils": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
