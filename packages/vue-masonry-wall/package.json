{
  "name": "@yeger/vue-masonry-wall",
  "version": "3.4.2",
  "description": "Responsive masonry layout with SSR support and zero dependencies for Vue 3.",
  "author": {
    "name": "Jan MÃ¼ller",
    "url": "https://github.com/DerYeger"
  },
  "license": "MIT",
  "homepage": "https://vue-masonry-wall.yeger.eu",
  "repository": "github:DerYeger/yeger",
  "bugs": {
    "url": "https://github.com/DerYeger/yeger/issues"
  },
  "keywords": [
    "vue",
    "plugin",
    "component",
    "masonry",
    "wall"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.umd.js",
  "browser": "dist/index.mjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "vite build",
    "bundlesize": "bundlesize",
    "dev": "vite build --watch",
    "lint": "yeger-lint",
    "test": "vitest",
    "check:tsc": "tsc --noEmit",
    "check:vue": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/types": "7.20.7",
    "@types/resize-observer-browser": "0.1.7",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitest/coverage-c8": "0.26.3",
    "@vitest/ui": "0.26.3",
    "@vue/test-utils": "2.2.6",
    "@yeger/tsconfig": "workspace:*",
    "bundlesize2": "0.0.31",
    "c8": "7.12.0",
    "flush-promises": "1.0.2",
    "jsdom": "20.0.3",
    "typescript": "4.9.4",
    "vite": "4.0.0",
    "vite-plugin-lib": "workspace:*",
    "vitest": "0.26.3",
    "vue": "3.2.45",
    "vue-tsc": "1.0.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
