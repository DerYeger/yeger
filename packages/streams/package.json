    "check:tsc": "tsc",
    "dev": "tsdown --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "@yeger/tsconfig": "workspace:*",
    "@yeger/vitest-utils": "workspace:*",
    "publint": "catalog:",
    "tsdown": "catalog:",
