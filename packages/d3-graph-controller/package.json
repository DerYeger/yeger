{
  "name": "d3-graph-controller",
  "version": "2.4.0",
  "description": "A TypeScript library for visualizing and simulating directed, interactive graphs.",
  "author": {
    "name": "Jan MÃ¼ller",
    "url": "https://github.com/DerYeger"
  },
  "license": "MIT",
  "homepage": "https://graph-controller.yeger.eu",
  "repository": "github:DerYeger/yeger",
  "bugs": {
    "url": "https://github.com/DerYeger/yeger/issues"
  },
  "keywords": [
    "d3",
    "graph",
    "controller"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/main.umd.js",
      "import": "./dist/main.mjs"
    },
    "./default.css": {
      "require": "./default.css",
      "import": "./default.css"
    }
  },
  "main": "./dist/main.umd.js",
  "module": "./dist/main.mjs",
  "types": "dist/types/main.d.ts",
  "files": [
    "dist",
    "default.css"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "yeger-lint",
    "test": "vitest",
    "check:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@yeger/debounce": "workspace:*",
    "d3-drag": "3.0.0",
    "d3-force": "3.0.0",
    "d3-selection": "3.0.0",
    "d3-zoom": "3.0.0",
    "vecti": "workspace:*"
  },
  "devDependencies": {
    "@types/d3-drag": "3.0.1",
    "@types/d3-force": "3.0.3",
    "@types/d3-selection": "3.0.3",
    "@types/d3-zoom": "3.0.1",
    "@types/node": "18.11.13",
    "@types/resize-observer-browser": "0.1.7",
    "@vitest/coverage-c8": "0.25.7",
    "@vitest/ui": "0.25.7",
    "@yeger/tsconfig": "workspace:*",
    "c8": "7.12.0",
    "jsdom": "20.0.3",
    "ts-deepmerge": "4.0.0",
    "typescript": "4.9.4",
    "vite": "4.0.0",
    "vite-plugin-lib": "workspace:*",
    "vitest": "0.25.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
