      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "check:publish": "publint run --strict",
    "check:tsc": "tsc",
    "lint": "yeger-lint"
  },
  "peerDependencies": {
    "typescript": "*",
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "picocolors": "1.0.0",
    "vite-plugin-dts": "3.3.1"
  },
  "devDependencies": {
    "@types/node": "18.17.1",
    "@yeger/tsconfig": "workspace:*",
    "typescript": "5.1.6",
    "unbuild": "v2.0.0-rc.0",
