{
  "name": "@yeger/fol",
  "version": "0.3.0",
  "description": "FOL grammar and parser.",
  "author": {
    "name": "Jan MÃ¼ller",
    "url": "https://github.com/DerYeger"
  },
  "license": "MIT",
  "homepage": "https://github.com/DerYeger/yeger/tree/main/packages/fol",
  "repository": "github:DerYeger/yeger",
  "bugs": {
    "url": "https://github.com/DerYeger/yeger/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/types/fol.d.ts",
        "default": "./dist/fol.umd.js"
      },
      "import": {
        "types": "./dist/types/fol.d.mts",
        "default": "./dist/fol.mjs"
      }
    }
  },
  "main": "./dist/fol.umd.js",
  "module": "./dist/fol.mjs",
  "types": "./dist/types/fol.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "check:publish": "publint run --strict",
    "check:tsc": "tsc",
    "codegen": "ohm generateBundles --withTypes 'src/*.ohm' --esm",
    "dev": "vite build --watch",
    "lint": "yeger-lint",
    "test": "vitest"
  },
  "dependencies": {
    "ohm-js": "17.1.0"
  },
  "devDependencies": {
    "@ohm-js/cli": "2.0.0",
    "@types/node": "18.17.1",
    "@yeger/tsconfig": "workspace:*",
    "resumon": "workspace:*",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-plugin-lib": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
