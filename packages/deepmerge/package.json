{
  "name": "@yeger/deepmerge",
  "version": "1.0.1",
  "private": true,
  "repository": "github:DerYeger/yeger",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "check:publish": "publint run --strict",
    "check:tsc": "tsc",
    "lint": "yeger-lint"
  },
  "devDependencies": {
    "@types/node": "18.17.1",
    "@yeger/tsconfig": "workspace:*",
    "typescript": "5.1.6",
    "unbuild": "v2.0.0-rc.0"
  }
}
