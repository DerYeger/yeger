---
import { Image } from 'astro:assets'

async function log() {
  // eslint-disable-next-line no-console
  console.log(Image)
  return Promise.resolve()
}

await log()
---

<div class="bg-blue text-red">Hello World!</div>

<script>
const test: number = 0
const setTitle = (value: number) => {
  document.title = `${value}`
}
document.onload = () => setTitle(test)
</script>
